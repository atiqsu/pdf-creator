{
    "name": "memtoko/pdf-creator",
    "type": "application",
    "description": "A beautifull syntax pdf creation for php.",
    "license": "MIT",
    "authors": [
        {
            "name": "Syaiful Bahri",
            "email": "syaiful@memtoko.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "dompdf/dompdf" : "0.6.*"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For pdf creation from url source."
    },
    "autoload": {
        "psr-4": {
            "Memtoko\\PdfCreator\\": "src"
        },
        "files": [
            "src/settings.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
